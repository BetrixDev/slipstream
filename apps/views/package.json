    "dotenv-cli": "^7.4.2",
    "env": "workspace:*",
    "fast-jwt": "^4.0.5",
    "hono": "^4.6.2",
    "ioredis": "^5.4.1",
    "nanoid": "^5.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.6.2",
    "tsx": "^4.19.1"
  }
}